body {
    font-family: "微软雅黑", tahoma, 'helvetica neue', arial, 'hiragino sans gb', 'Simsun', sans-serif;
    margin: 0px;
    padding: 0px;
    font-size: 14px;
    color: #555;
    -webkit-font-smoothing: subpixel-antialiased;
}

.main {}

.console {
    display: -ms-flexbox;
    display: -webkit-flex;
    display: -moz-flex;
    display: -ms-flex;
    display: flex;
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    overflow: hidden;
}

.apparea {
    height: 100%;
    -webkit-flex: 1;
    -moz-flex: 1;
    -ms-flex: 1;
    flex: 1;
}

.container {
    background-color: #fff;
    overflow: hidden;
    height: 100%;
}

.pane {
    min-width: 800px;
    height: 100%;
    padding: 60px 20px;
}

.toolbar {
    padding: 20px 0;
}

.header {
    z-index: 10;
    width: 100%;
    height: 50px;
    position: fixed;
    background-color: #fff;
}

.header .logo-area {
    background-color: #2c3e50;
    padding: 10px 0;
    float: left;
}

.header .logo {
    float: left;
    width: 200px;
    height: 30px;
    margin: 0;
    background-image: url("../images/logo-01.png");
    background-position: center center;
    background-repeat: no-repeat;
    background-size: contain;
}

.header-inner {
    height: 48px;
    margin-left: 200px;
    -webkit-box-shadow: 0 -1px 8px 1px #bbb;
    -moz-box-shadow: 0 -1px 8px 1px #bbb;
    box-shadow: 0 -1px 8px 1px #bbb;
}

.header .top-nav-menu {
    background: url(../images/logo-nav.png) no-repeat scroll center center rgba(0, 0, 0, 0);
    *background-image: url("../images/nav-menu.png");
    background-image: url("../images/nav-menu.png") \0;
    background-attachment: scroll;
    background-repeat: no-repeat;
    background-position: center center;
    -ms-background-position-x: center;
    -ms-background-position-y: center;
    background-size: auto;
    background-origin: padding-box;
    background-clip: border-box;
    background-color: transparent;
    display: block;
    height: 30px;
    overflow: hidden;
    width: 30px;
    float: left;
    margin: 10px 20px;
}

.header-account {
    float: right;
    font-size: 12px;
    margin-right: 20px;
}

.header-account li {
    display: inline-block;
    line-height: 25px;
}

.header-account li a {
    display: block;
    padding: 12px 20px;
    border-left: 1px solid #f4f4f4;
    color:#555;
}

.header-account .account-item {
    margin-left: -4px;
    position: relative;
    top: 0;
    left: 0;
}

.header-account .account-item:hover{
    background-color: #f8f8f8;
}

.header-account .account-item:hover .more {
    display: block;
}

.header-account .pool li a{
    display: block;
    width: 100%;
}

.header-account .pool li:hover{
    background-color: #f8f8f8;
}

.header-account .ticket {
    
}

.header-account .ticket li{
    width: 300px;
    float: left;
}

.header-account .ticket .ticket-index{
    background-color: #f8f8f8;
}

.header-account .ticket .ticket-index b{
    margin: 0 5px;
}

.header-account .ticket .ticket-index span{
    display: block;
    padding: 10px 20px;
}

.header-account .ticket .ticket-content li{
    border-bottom: 1px solid #f4f4f4;
}

.header-account .ticket .ticket-content p{
    padding: 10px 20px;
    margin: 0;
}

.header-account .ticket .ticket-content p.ticket-title{
    color: #333;
    font-weight: bold;
}

.header-account .ticket .ticket-content p.ticket-title span{

}

.header-account .ticket .ticket-new {
    padding: 5px 10px;
    margin: 10px 20px;
    border: none;
    float: left;
}

.header-account .ticket .ticket-all {
    padding: 5px;
    margin: 10px 20px;
    border: none;
    float: right;
}

.header-account .user-item {
    border-right: 1px solid #f4f4f4;
}
.header-account .user li{
  width:100%;
}
.header-account .user li a{
    display: block;
    width: 100%;
}

.header-account .user li:hover {
    background-color: #f8f8f8;
}

.header-account .more {
    display: none;
    box-shadow: 0 0 3px 0 #bbb;
    position: absolute;
    background: #fff;
    z-index: 3;
}

.header-account .more li{
    line-height: 18px;
}

.sidebar {
    min-width: 200px;
    max-width: 200px;
    position: relative;
    overflow: hidden;
    background-color: #34495e;
    -webkit-flex: 1;
    -moz-flex: 1;
    -ms-flex: 1;
    flex: 1;
}
.sidemenu{
    padding: 80px 0;
}
.sidemenu .menu-item{
    margin: 0;
    line-height: 45px;
}
.sidemenu .menu-item dt {
    color: #ddd;
    font-size: 14px;
    text-indent: 50px;
    position: relative;
    cursor: pointer;
    font-weight: normal;
}
.sidemenu .menu-item dt a {
    display: block;
    text-align: left;
    font-size: 14px;
    color: #ddd;
    line-height: 45px;
}

.sidemenu .menu-item dt:hover {
    background-color: #2980b9;
}

.sidemenu .menu-item dd {
    margin: 0;
    line-height: 40px;
    position: relative;
}

.sidemenu .menu-item dd a {
    display: block;
    text-decoration: none;
    color: #fff;
    padding: 0 0 0 65px;
}

.sidemenu .menu-item dd a:hover {
    background-color: #2980b9;
}
.sidemenu .menu-item dd i{
    background-repeat: no-repeat;
    -ms-background-position-x: left;
    -ms-background-position-y: 13px;
    background-size: 16px;
    background-origin: padding-box;
    background-clip: border-box;
    background-color: transparent;
    display: block;
    float: left;
    width: 20px;
    height: 35px;
}
