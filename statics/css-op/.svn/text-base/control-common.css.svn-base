.table,
.modal-table {
    background-color: #fff;
    border-collapse: collapse;
    border-spacing: 0;
    width: 100%;
    color: #333;
    font-size: 12px;
}

.table thead th,
.modal-table thead th {
    padding: 12px;
    text-align: left;
    white-space: nowrap;
    word-break: keep-all;
    color: #555;
    cursor: pointer;
    font-weight: normal;
    background: #cfeaf8;
}

.table thead th:first-child,
.modal-table thead th:first-child {
    width: 20px;
}

.table tbody td,
.modal-table tbody td {
    border-bottom: 1px solid #f4f5f6;
    padding: 12px;
    text-align: left;
    word-wrap: break-word;
}

.table tbody tr:hover,
.modal-table tbody tr:hover {
    background-color: #e7f7ff;
}
.table .select_div .blue:hover{
  text-decoration:none;
}
.table .select_div{
